@font-face {
  font-family: "Font Awesome 5 Brands";
  font-style: normal;
  font-weight: 400;
  font-display: block;
  src: url(<%= asset_path('fa-brands-400.eot') %>);
  src: url(<%= asset_path('fa-brands-400.eot') %>?#iefix) format("embedded-opentype"),
    url(<%= asset_path('fa-brands-400.woff2') %>) format("woff2"),
    url(<%= asset_path('fa-brands-400.woff') %>) format("woff"),
    url(<%= asset_path('fa-brands-400.ttf') %>) format("truetype"),
    url(<%= asset_path('fa-brands-400.svg') %>#fontawesome) format("svg")
}

